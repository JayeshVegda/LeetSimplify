{
    "projectType": "chrome-extension",
    "primaryLanguage": "javascript",
    "entryPoints": [
      "content.js",
      "popup.js",
      "background.js"
    ],
    "apiTargets": [
      "https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent"
    ],
    "description": "Chrome extension that simplifies LeetCode problem descriptions using Gemini API.",
    "documentation": [
      "README.md"
    ],
    "structure": {
      "manifest": "manifest.json",
      "contentScript": "content.js",
      "popup": {
        "html": "popup.html",
        "script": "popup.js"
      },
      "styles": "styles.css"
    },
    "rules": {
      "linting": true,
      "formatting": true,
      "typeCheck": false
    }
  }
  